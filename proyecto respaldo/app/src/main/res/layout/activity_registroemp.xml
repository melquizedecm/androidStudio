<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo_kuiny"
    tools:context=".registroemp">

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="match_parent">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="39dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="168dp"
                android:text="Contraseña"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView13" />

            <EditText
                android:id="@+id/contrasenia"
                android:layout_width="180dp"
                android:layout_height="39dp"
                android:layout_marginStart="80dp"
                android:layout_marginTop="100dp"
                android:layout_marginEnd="20dp"
                android:ems="10"
                android:hint="Contraseña"
                android:inputType="textPassword"
                android:maxLength="13"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/textView15"
                app:layout_constraintTop_toBottomOf="@+id/correoempresa" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="39dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="29dp"
                android:text="Correo Empresarial"
                android:textSize="18sp"
                app:layout_constraintEnd_toStartOf="@+id/correoempresa"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView13" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="147dp"
                android:layout_height="44dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="92dp"
                android:text="Usuario de la empresa"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView13" />

            <EditText
                android:id="@+id/correoempresa"
                android:layout_width="180dp"
                android:layout_height="39dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="104dp"
                android:layout_marginEnd="42dp"
                android:ems="10"
                android:hint="Correo Empresarial"
                android:inputType="textEmailAddress"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/textView14"
                app:layout_constraintTop_toBottomOf="@+id/textView12" />

            <EditText
                android:id="@+id/userempresa"
                android:layout_width="171dp"
                android:layout_height="39dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="172dp"
                android:layout_marginEnd="8dp"
                android:ems="10"
                android:hint="Usuario de la empresa"
                android:inputType="text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.397"
                app:layout_constraintStart_toEndOf="@+id/textView14"
                app:layout_constraintTop_toBottomOf="@+id/textView12" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="39dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="36dp"
                android:text="Telefono"
                android:textSize="18sp"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView12" />

            <EditText
                android:id="@+id/telefono"
                android:layout_width="180dp"
                android:layout_height="39dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:hint="Telefono"
                android:maxLength="10"
                android:inputType="phone"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/encargado" />

            <EditText
                android:id="@+id/encargado"
                android:layout_width="180dp"
                android:layout_height="46dp"
                android:layout_marginStart="96dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="45dp"
                android:ems="10"
                android:hint="Encargado de la actividad"
                android:inputType="text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/textView12"
                app:layout_constraintTop_toBottomOf="@+id/rfc" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="145dp"
                android:layout_height="49dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="36dp"
                android:text="Encargado de la actividad"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView11" />

            <EditText
                android:id="@+id/rfc"
                android:layout_width="180dp"
                android:layout_height="39dp"
                android:layout_marginStart="128dp"
                android:layout_marginTop="23dp"
                android:layout_marginEnd="45dp"
                android:ems="10"
                android:hint="R.F.C"
                android:inputType="text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/textView11"
                app:layout_constraintTop_toBottomOf="@+id/nombreempresa" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="39dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="24dp"
                android:maxLength="13"
                android:text="R.F.C"
                android:textSize="18sp"
                app:layout_constraintEnd_toStartOf="@+id/rfc"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView9" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="315dp"
                android:layout_height="49dp"
                android:layout_marginStart="63dp"
                android:layout_marginTop="48dp"
                android:layout_marginEnd="622dp"
                android:text="REGISTRO DE EMPRESAS"
                android:textSize="25sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.033"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="39dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="36dp"
                android:layout_marginEnd="11dp"
                android:text="Nombre de empresa"
                android:textSize="18sp"
                app:layout_constraintEnd_toStartOf="@+id/nombreempresa"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView8" />

            <EditText
                android:id="@+id/nombreempresa"
                android:layout_width="180dp"
                android:layout_height="39dp"
                android:layout_marginStart="9dp"
                android:layout_marginTop="39dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:hint="Nombre de empresa"
                android:inputType="text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/textView9"
                app:layout_constraintTop_toBottomOf="@+id/textView8" />

            <CheckBox
                android:id="@+id/chec"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="65dp"
                android:layout_marginTop="38dp"
                android:layout_marginEnd="113dp"
                android:text="Acepto Terminos y condiciones"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView15"
                app:layout_constraintVertical_chainStyle="spread_inside" />

            <Button
                android:id="@+id/acept"
                android:layout_width="115dp"
                android:layout_height="61dp"
                android:layout_marginStart="124dp"
                android:layout_marginTop="56dp"
                android:layout_marginEnd="172dp"
                android:background="@drawable/continuar"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/chec" />
        </android.support.constraint.ConstraintLayout>
    </ScrollView>

</LinearLayout>
